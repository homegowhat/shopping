<!DOCTYPE html>
<html>
<head>
<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />

<link rel="stylesheet" href="css/jquery.fileupload-ui.css" />
<script src="js/vendor/jquery.ui.widget.js"></script>
<script src="js/jquery.iframe-transport.js"></script>
<script src="js/jquery.fileupload.js"></script>
</head>



<body>
<div  rabbit:class="com.kuroshio.web.module.picture.page.PictureAddPage" websiteId="homegowhat" type="slide">
     		<form method="post"></form>
	<div  width="800" height="500" title="上傳圖片">
    
    	<div class="container">
   			 <span class="btn btn-success fileinput-button">
       			 <i class="icon-plus icon-white"></i>
        		<span>選取批次上傳的圖片</span>
        			<input id="fileupload" type="file" name="files[]" multiple="multiple" />
    			</span>
    		<br />
   			 <br />

   			 <div id="progress" class="progress progress-success progress-striped">
        		<div class="bar"></div>
   			 </div>

    		<div id="files"></div>    
		</div>
<script type="text/javascript">
$(function () {
   // var url = './../../upload?type=SLIDE';
    var url = './wedding/x.upload?type=SLIDE';
    $('#fileupload').fileupload({
        autoUpload: true,
        url: url,
        dataType: 'json',
        done: function (e, data) {
            $.each(data.result.files, function (index, file) {//
               // $('<p/>').text(file.name).appendTo('#files');
                $('<img width="150" height="150" />').attr("src","./wedding/x.Pic?t=SLIDE&id="+file.name).appendTo('#files');
            });
        },
        progressall: function (e, data) {
            var progress = parseInt(data.loaded / data.total * 100, 10);
            $('#progress .bar').css(
                'width',
                progress + '%'
            );
        }
    });
});
</script>
		</div>

</div>
</body>
</html>
